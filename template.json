    "layer0": {
      "type": "number",
      "default": 1
    },
    "layer1": {
      "type": "number",
      "default": 1
    },
    "layer2": {
      "type": "number",
      "default": 1
    },
    "layer3": {
     "type": "number",
     "default": 1
    },
    "layer4": {
      "type": "number",
      "default": 1
    },
    "layer5": {
     "type": "number",
     "default": 1
    },
    "layer6": {
     "type": "number",
